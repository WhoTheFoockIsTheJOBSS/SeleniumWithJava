<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite Groups">
    <test name="Group Smoke Test">
        <groups>
            <run>
                <include name="Smoke"/>
            </run>
        </groups>
        <classes>
            <class name="Ejemplos.modulo1.ItestContextDataProvider"></class>
        </classes>
    </test>

    <test name="Group Regression Test">
        <groups>
            <run>
                <include name="Regression"/>
            </run>
        </groups>
        <classes>
            <class name="Ejemplos.modulo1.ItestContextDataProvider"></class>
        </classes>
    </test>
</suite>